month = {jul},
number = {1},
pages = {10--15},
title = {{Ictal SPECT analysis in epilepsy: subtraction and statistical parametric mapping techniques.}},
url = {http://www.neurology.org/cgi/doi/10.1212/01.WNL.0000132885.83350.45 http://www.ncbi.nlm.nih.gov/pubmed/15249603},
