month = {aug},
number = {8},
pages = {785--9},
pmid = {22854929},
title = {{Designing case-control studies: decisions about the controls.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3921907{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {169},
year = {2012}
}
