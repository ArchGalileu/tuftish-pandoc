journal = {Schizophrenia Research},
keywords = {20th Century,Bibliometrics,History,Humans,Models,Psychological,PubMed,PubMed: statistics {\&} numerical data,Schizophrenia,Schizophrenia: history,Schizophrenic Psychology,United States},
month = {mar},
number = {1-3},
