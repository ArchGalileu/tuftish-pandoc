journal = {Psychoneuroendocrinology},
keywords = {Control persons,Cortisol,Depression,Meta-analysis,Saliva,Systematic review,aol,com,deposition profile along a,from a reservoir,http,jamessp8,manuscriptcentral,mc,prediction of the asphaltene,ueso email,url,wellbore during natural production},
month = {oct},
