issn = {0022-3999},
journal = {Journal of psychosomatic research},
keywords = {Dementia,Dementia: diagnosis,Dementia: therapy,Frontal Lobe,Frontal Lobe: pathology,Humans,Magnetic Resonance Imaging,Male,Middle Aged,Neuropsychological Tests,Personality Development,Social Behavior,Social Perception,Temporal Lobe,Temporal Lobe: pathology,frontal lobes,frontotemporal dementia,social cognition,theory of mind},
