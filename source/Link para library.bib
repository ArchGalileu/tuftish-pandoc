pmid = {21886302},
title = {{Koch's postulates, carnivorous cows, and tuberculosis today.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3158372{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {70},
year = {2011}
}
