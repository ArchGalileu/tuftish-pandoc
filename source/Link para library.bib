title = {{Assessing the Level of Structural Integration Using Operationalized Psychodynamic Diagnosis ( OPD ): Implications for DSM – 5}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22808938},
volume = {94},
year = {2012}
}
