journal = {Int J Geriatr Psychiatr},
keywords = {accepted 12 july 2012,brazilian sample,cognitive performance,com,history,major depression and older,online library,patients,predictive factors,published online in wiley,received 20 march 2012,remission,response,treatment algorithm,wileyonlinelibrary},
month = {jun},
number = {6},
pages = {647--53},
pmid = {23015472},
title = {{Validation of a treatment algorithm for major depression in an older Brazilian sample.}},
