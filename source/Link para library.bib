month = {dec},
number = {3},
pages = {273--82},
pmid = {17179997},
publisher = {Nature Publishing Group},
title = {{Interleukin 3 and schizophrenia: the impact of sex and family history.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17179997 http://www.nature.com/doifinder/10.1038/sj.mp.4001932},
