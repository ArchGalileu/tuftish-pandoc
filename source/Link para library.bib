isbn = {0920-9964$\backslash$r1573-2509},
issn = {15732509},
journal = {Schizophrenia Research},
keywords = {First episode psychosis,Psychosis course,Substance use},
number = {1-3},
pages = {60--63},
pmid = {24525084},
publisher = {Elsevier B.V.},
title = {{The impact of substance use at psychosis onset on First Episode Psychosis course: Results from a 1 year follow-up study in Bologna}},
url = {http://dx.doi.org/10.1016/j.schres.2014.01.014},
volume = {153},
year = {2014}
}
