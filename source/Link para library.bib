keywords = {Adolescent,Adult,Analysis of Variance,Computer-Assisted,Depressive Disorder,Female,Frontal Lobe,Frontal Lobe: pathology,Functional Laterality,Humans,Image Processing,Limbic System: pathology,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Major,Major: complications,Major: pathology,Middle Aged,affective disorders,depression,imaging,limbic system,magnetic resonance,mood disorders,suicide},
month = {apr},
