@article{Cartwright2010,
abstract = {What kinds of evidence reliably support predictions of effectiveness for health and social care interventions? There is increasing reliance, not only for health care policy and practice but also for more general social and economic policy deliberation, on evidence that comes from studies whose basic logic is that of JS Mill's method of difference. These include randomized controlled trials, case-control studies, cohort studies, and some uses of causal Bayes nets and counterfactual-licensing models like ones commonly developed in econometrics. The topic of this paper is the 'external validity' of causal conclusions from these kinds of studies. We shall argue two claims. Claim, negative: external validity is the wrong idea; claim, positive: 'capacities' are almost always the right idea, if there is a right idea to be had. If we are right about these claims, it makes big problems for policy decisions. Many advice guides for grading policy predictions give top grades to a proposed policy if it has two good Mill's-method-of difference studies that support it. But if capacities are to serve as the conduit for support from a method-of-difference study to an effectiveness prediction, much more evidence, and much different in kind, is required. We will illustrate the complexities involved with the case of multisystemic therapy, an internationally adopted intervention to try to diminish antisocial behaviour in young people.},
author = {Cartwright, Nancy and Munro, Eileen},
doi = {10.1111/j.1365-2753.2010.01382.x},
file = {::},
issn = {1365-2753},
journal = {Journal of evaluation in clinical practice},
keywords = {Antisocial Personality Disorder,Antisocial Personality Disorder: therapy,Forecasting,Health Policy,Humans,Models,Policy Making,Randomized Controlled Trials as Topic,Randomized Controlled Trials as Topic: standards,Theoretical},
month = {apr},
number = {2},
pages = {260--6},
pmid = {20367845},
title = {{The limitations of randomized controlled trials in predicting effectiveness.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20367845},
volume = {16},
year = {2010}
}
