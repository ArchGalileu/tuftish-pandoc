number = {6},
pages = {457--467},
pmid = {16039619},
title = {{The relationship of structural alterations to cognitive deficits in schizophrenia: a voxel-based morphometry study}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16039619{\%}5Cnhttp://ac.els-cdn.com/S000632230500497X/1-s2.0-S000632230500497X-main.pdf?{\_}tid=704971fb5ff28c3c34ab91222ab0e12e{\&}acdnat=1343409653{\_}e3b91b5caf33088e016424eed39f7847},
