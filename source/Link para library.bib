pages = {75--84},
pmid = {15572276},
title = {{Acute and continuation risperidone monotherapy in bipolar mania: a 3-week placebo-controlled trial followed by a 9-week double-blind trial of risperidone and haloperidol.}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve{\%}7B{\&}{\%}7Ddb=PubMed{\%}7B{\&}{\%}7Ddopt=Citation{\%}7B{\&}{\%}7Dlist{\%}7B{\_}{\%}7Duids=15572276},
