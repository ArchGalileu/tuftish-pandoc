@article{Fusar-Poli2012,
abstract = {BACKGROUND: Reliable neurofunctional markers of increased vulnerability to psychosis are needed to improve the predictive value of psychosis risk syndrome and inform preventive interventions.{\$}\backslash{\$}n{\$}\backslash{\$}nMETHODS: I performed a signed differential mapping (SDM) voxel-wise meta-analysis of functional magnetic resonance imaging (fMRI) studies of patients at clinical high risk for psychosis.{\$}\backslash{\$}n{\$}\backslash{\$}nRESULTS: Ten studies were included in the analysis. Compared with controls, high-risk patients showed reduced neural activation in the left inferior frontal gyrus (Brodmann area [BA] 9) and in a cluster spanning the bilateral medial frontal gyrus (BA 8,6), bilateral superior frontal gyrus (BA 8,6)and the left anterior cingulate (BA 32). There was no publication bias. Heterogeneity across studies was low. Sensitivity analysis confirmed the robustness of the findings.{\$}\backslash{\$}n{\$}\backslash{\$}nLIMITATIONS: The cross-sectional nature of the included studies prevented the comparison of high-risk patients who later experienced a psychotic episode with those who did not. Other caveats are reflected in methodologic heterogeneity across tasks employed by different individual imaging studies.{\$}\backslash{\$}n{\$}\backslash{\$}nCONCLUSION: Reduced neurofunctional activation in prefrontal regions may represent a neurophysiologic correlate of increased vulnerability to psychosis.},
author = {Fusar-Poli, Paolo},
doi = {10.1503/jpn.110021},
