keywords = {Mania - tto duas fases,affective disorder,antipsychotics,bipolar disorder,efficacy,haloperidol,long term,mania,mood stabilizers,multicenter,randomized controlled trial,rating scale,risperidone,safety,tardive dyskinesia},
mendeley-tags = {Mania - tto duas fases},
