journal = {World psychiatry : official journal of the World Psychiatric Association (WPA)},
month = {feb},
number = {1},
pages = {28--35},
pmid = {24497240},
title = {{The RDoC framework: facilitating transition from ICD/DSM to dimensional approaches that integrate neuroscience and psychopathology.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3918011{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {13},
