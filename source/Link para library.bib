month = {mar},
number = {3},
pages = {258--75},
pmid = {15611786},
title = {{Obsessive-compulsive disorder phenotypes: implications for genetic studies.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15611786},
volume = {10},
year = {2005}
}
