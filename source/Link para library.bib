journal = {The British Journal Of Psychiatry Supplement},
number = {51},
pages = {s69----s75},
pmid = {18055941},
title = {{Structural brain abnormalities in individuals with an at-risk mental state who later develop psychosis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18055941},
volume = {51},
year = {2007}
}
