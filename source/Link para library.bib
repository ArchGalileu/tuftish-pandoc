keywords = {Early course,Early initial prodromal state,Late initial prodromal state,Phenomenology,Psychosis},
number = {1},
pages = {182--191},
pmid = {18579555},
title = {{Basic symptoms and ultrahigh risk criteria: Symptom development in the initial prodromal state}},
volume = {36},
year = {2010}
}
