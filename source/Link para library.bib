journal = {International journal of geriatric psychiatry},
keywords = {Age Factors,Aged,Brazil,Brazil: epidemiology,Female,Geriatric Psychiatry,Health Surveys,Humans,Male,Mental Disorders,Mental Disorders: epidemiology,Mental Health Services,Middle Aged,Prevalence,Primary Health Care,Primary Health Care: statistics {\&} numerical data,Risk Factors,Social Class},
