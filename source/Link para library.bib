journal = {Schizophrenia Research},
keywords = {Attention,Schizophrenia,Verbal fluency,Working memory},
number = {1-3},
pages = {338--343},
pmid = {17123787},
title = {{Cognitive deficits in first-episode psychosis: A population-based study in S{\~{a}}o Paulo, Brazil}},
volume = {90},
year = {2007}
}
