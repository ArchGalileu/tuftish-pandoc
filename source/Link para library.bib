@article{DiForti2014,
abstract = {Cannabis use is associated with an earlier age of onset of psychosis (AOP). However, the reasons for this remain debated. Methods: We applied a Cox proportional hazards model to 410 first-episode psychosis patients to investigate the association between gender, patterns of cannabis use, and AOP. Results: Patients with a history of cannabis use presented with their first episode of psychosis at a younger age (mean years = 28.2, SD = 8.0; median years = 27.1) than those who never used cannabis (mean years = 31.4, SD = 9.9; median years = 30.0; hazard ratio [HR] = 1.42; 95{\%} CI: 1.16–1.74; P {\textless} .001). This association remained significant after controlling for gender (HR = 1.39; 95{\%} CI: 1.11–1.68; P {\textless} .001). Those who had started cannabis at age 15 or younger had an earlier onset of psychosis (mean years = 27.0, SD = 6.2; median years = 26.9) than those who had started after 15 years (mean years = 29.1, SD = 8.5; median years = 27.8; HR = 1.40; 95{\%} CI: 1.06–1.84; P = .050). Importantly, subjects who had been using high-potency cannabis (skunk-type) every day had the earliest onset (mean years = 25.2, SD = 6.3; median years = 24.6) compared to never users among all the groups tested (HR = 1.99; 95{\%} CI: 1.50- 2.65; P {\textless} .0001); these daily users of high-potency cannabis had an onset an average of 6 years earlier than that of non-cannabis users. Conclusions: Daily use, especially of high-potency cannabis, drives the earlier onset of psychosis in cannabis users.},
author = {{Di Forti}, Marta and Sallis, Hannah and Allegri, Fabio and Trotta, Antonella and Ferraro, Laura and Stilo, Simona A. and Marconi, Arianna and {La Cascia}, Caterina and Marques, Tiago Reis and Pariante, Carmine and Dazzan, Paola and Mondelli, Valeria and Paparelli, Alessandra and Kolliakou, Anna and Prata, Diana and Gaughran, Fiona and David, Anthony S. and Morgan, Craig and Stahl, Daniel and Khondoker, Mizanur and MacCabe, James H. and Murray, Robin M.},
doi = {10.1093/schbul/sbt181},
file = {::},
isbn = {0044207848010},
issn = {17451701},
journal = {Schizophrenia Bulletin},
keywords = {age of onset,cannabis,drug use,gender,high-potency cannabis,psychotic disorders,survival plots},
number = {6},
pages = {1509--1517},
pmid = {24345517},
title = {{Daily use, especially of high-potency cannabis, drives the earlier onset of psychosis in cannabis users}},
volume = {40},
year = {2014}
}
