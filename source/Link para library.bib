journal = {Schizophrenia research},
keywords = {Assessment,Attenuated psychosis syndrome,Clinical high-risk,Prodrome,Schizophrenia,Screening},
number = {2-3},
pages = {116--122},
pmid = {25975827},
publisher = {Elsevier B.V.},
title = {{Longitudinal validation of psychosis risk screening tools.}},
url = {http://www.sciencedirect.com/science/article/pii/S0920996415002212},
volume = {165},
year = {2015}
}
