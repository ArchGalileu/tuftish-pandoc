month = {aug},
number = {7565},
pages = {413},
pmid = {16849365},
title = {{Systematic reviews of diagnostic tests in cancer: review of methods and reporting.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1553548{\%}7B{\&}{\%}7Dtool=pmcentrez{\%}7B{\&}{\%}7Drendertype=abstract http://www.ncbi.nlm.nih.gov/pubmed/16849365},
