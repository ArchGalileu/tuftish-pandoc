@article{Castro-de-Araujo2014,
abstract = {Objectives: We performed a brief analysis of the concept of causality as applied to psychiatry and put forward an approach based on the notions of weak causal influences and complex causal networks. Method: a conceptual analysis of selected articles and books. Results: We present the INUS causation model proposed by the philosopher J. L. Mackie that circumvents some of the difficulties related to the presence of multiple interdependent causes, such as it appears to occur in the case of mental disorders. This model seemingly works well in PTSD, as shown. Conclusion: We suggest that the failure to find 'strong' causes for mental disorders may indicate the need to revise our expectations, with the adoption of alternative models of causality.},
author = {Castro-de-Araujo, Luis Fernando Silva and Banzato, Claudio E M},
