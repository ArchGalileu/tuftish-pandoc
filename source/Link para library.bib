month = {jan},
number = {3},
pages = {427--44},
pmid = {14609238},
title = {{Schizophrenia, consciousness, and the self.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14609238},
volume = {29},
year = {2003}
}
