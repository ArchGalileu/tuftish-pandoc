eprint = {NIHMS150003},
file = {::},
isbn = {1832-4274 (Print)$\backslash$r1832-4274 (Linking)},
issn = {1832-4274},
journal = {Twin Research and Human Genetics},
keywords = {20 years in,brain structure,exploring,healthy volunteers,herald the onset of,heritability,independent components analysis,schizophre-,schizophrenia,studies of schizophrenia and,the structural changes that,used for at least,volumetric analysis has been},
