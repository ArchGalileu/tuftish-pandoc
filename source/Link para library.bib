@article{Sadowsky2006,
abstract = {It is common to represent the history of American psychiatry by comparing it to a pendulum. In this metaphorical rendering, psychiatry has swung back and forth between extremes of emphasis on psyche (life events and inner conflict ruling etiological thinking, talk therapy dominating treatment) and soma (biochemical sources dominating etiological thinking, somatic treatments dominating treatment). This article argues that, while this metaphor is suited to capturing certain aspects of American psychiatric history, it distorts others by, for example, exaggerating the extent of dogmatism on either side and obscuring continuities in psychiatry's history. The article looks specifically at the reception by psychoanalysts to the introduction of electroconvulsive therapy. It shows that psychoanalytic views of ECT were diverse and more receptive to ECT than the pendulum metaphor might lead us to believe.},
author = {Sadowsky, Jonathan},
doi = {10.1093/jhmas/jrj001},
issn = {00225045},
journal = {Journal of the History of Medicine and Allied Sciences},
keywords = {Electroconvulsive therapy,Metaphor,Psychiatry,Psychoanalysis,Somatic treatments},
number = {1},
pages = {1--25},
pmid = {16239498},
title = {{Beyond the Metaphor of the Pendulum: Electroconvulsive Therapy, Psychoanalysis, and the Styles of American Psychiatry}},
volume = {61},
year = {2006}
}
@article{Melby2009,
abstract = {The real issue is one of "discomfort with one's assigned gender or the profound feeling that one is in the wrong gender." Charles Moser, MD, PhD proposes the name Gender Dysphoria Disorder (GDD), which would only apply to those experiencing "persistent and profound discomfort." That may remove some of the stigma because transgenders not experiencing discomfort wouldn't be diagnosed with a mental disorder. "We need to help this committee that's a pretty conservative committee to shift language so it's less pathologizing and less sexist," Lev says. "If you look at the criteria, you may be surprised to learn that boys who don't engage in 'rough and tumble play' may have GID. That's an absurd criteria to include at this point in the world." "First of all, it is not going to happen because it would be a public relations disaster for psychiatry," Robert Spitzer writes. "There was already a little disaster when the initial DSM-IV put in the 'clinical significance' criterion that had the effect of requiring distress or impairment before pedophilia could be diagnosed. The APA wisely corrected that in the DSM-IV-TR."},
author = {Melby, Todd},
journal = {Contemporary Sexuality},
number = {3},
