number = {1-7},
pages = {107--117},
pmid = {726238533241861127},
publisher = {Elsevier},
series = {WWW7},
title = {{The anatomy of a large-scale hypertextual Web search engine}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S016975529800110X},
