number = {3},
pages = {247--252},
pmid = {10981157},
publisher = {Medical Service (III), Department of Veterans Affairs NCHCS, 10535 Hospital Way, Mather, CA 95655, USA. siegel.david@martinez.va.gov},
title = {{The influence of national guidelines on antihypertensive prescribing patterns.}},
