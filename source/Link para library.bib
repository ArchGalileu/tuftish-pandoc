@article{Kochunov2017,
abstract = {{\textless}h3{\textgreater}Importance{\textless}/h3{\textgreater}{\textless}p{\textgreater}Efforts to remediate the multiple cognitive function impairments in schizophrenia should consider white matter as one of the underlying neural mechanisms.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Objective{\textless}/h3{\textgreater}{\textless}p{\textgreater}To determine whether altered structural brain connectivity is responsible for 2 of the core cognitive deficits in schizophrenia— reduced information processing speed and impaired working memory.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Design, Setting, and Participants{\textless}/h3{\textgreater}{\textless}p{\textgreater}This cross-sectional study design took place in outpatient clinics from August 1, 2004, to August 31, 2015. Participants included 166 patients with schizophrenia and 213 healthy control individuals. These participants were from 3 independent cohorts, each of which had its own healthy control group. No participant had current or past neurological conditions or major medical conditions. Patients were diagnosed with either schizophrenia or schizoaffective disorder as defined by the{\textless}i{\textgreater}DSM-IV{\textless}/i{\textgreater}. Controls had no Axis I psychiatric disorder.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Main Outcomes and Measures{\textless}/h3{\textgreater}{\textless}p{\textgreater}Mediation analyses and structural equation modeling were used to analyze the associations among processing speed, working memory, and white matter microstructures. Whole-brain and regional diffusion tensor imaging fractional anisotropy were used to measure white matter microstructures.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Of the study participants, the 166 patients with schizophrenia had a mean (SD) age of 38.2 (13.3) years and the 213 healthy controls had a mean (SD) age of 39.2 (14.0) years. There were significantly more male patients than controls in each of the 3 cohorts (117 [70{\%}] vs 91 [43{\%}]), but there were no significant differences in sex composition among the 3 cohorts. Patients had significantly reduced processing speed (Cohen{\textless}i{\textgreater}d{\textless}/i{\textgreater} = 1.24;{\textless}i{\textgreater}P{\textless}/i{\textgreater} = 6.91 × 10{\textless}sup{\textgreater}−30{\textless}/sup{\textgreater}) and working memory deficits (Cohen{\textless}i{\textgreater}d{\textless}/i{\textgreater} = 0.83;{\textless}i{\textgreater}P{\textless}/i{\textgreater} = 1.10 × 10{\textless}sup{\textgreater}−14{\textless}/sup{\textgreater}) as well as a significant whole-brain fractional anisotropy deficit (Cohen{\textless}i{\textgreater}d{\textless}/i{\textgreater} = 0.63;{\textless}i{\textgreater}P{\textless}/i{\textgreater} = 2.20 × 10{\textless}sup{\textgreater}−9{\textless}/sup{\textgreater}). In schizophrenia, working memory deficit was mostly accounted for by processing speed deficit, but this deficit remained when accounting for working memory (Cohen{\textless}i{\textgreater}d{\textless}/i{\textgreater} = 0.89;{\textless}i{\textgreater}P{\textless}/i{\textgreater} = 2.21 × 10{\textless}sup{\textgreater}−17{\textless}/sup{\textgreater}). Mediation analyses showed a significant association pathway from fractional anisotropy to processing speed to working memory ({\textless}i{\textgreater}P{\textless}/i{\textgreater} = 5.01 × 10{\textless}sup{\textgreater}−7{\textless}/sup{\textgreater}). The strength of this brain-to-cognition pathway in different white matter tracts was strongly associated with the severity of schizophrenia-associated fractional anisotropy deficits in the corresponding white matter tracts as determined by a meta-analysis ({\textless}i{\textgreater}r{\textless}/i{\textgreater} = 0.85-0.94; all{\textless}i{\textgreater}P{\textless}/i{\textgreater} {\textless} .001). The same pattern was observed in patients and controls either jointly or independently.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions and Relevance{\textless}/h3{\textgreater}{\textless}p{\textgreater}Study findings suggest that (1) processing speed contributes to the association between white matter microstructure and working memory in schizophrenia and (2) white matter impairment in schizophrenia is regional tract–specific, particularly in tracts normally supporting processing speed performance.{\textless}/p{\textgreater}},
author = {Kochunov, Peter and Coyle, Thomas R. and Rowland, Laura M. and Jahanshad, Neda and Thompson, Paul M. and Kelly, Sinead and Du, Xiaoming and Sampath, Hemalatha and Bruce, Heather and Chiappelli, Joshua and Ryan, Meghann and Fisseha, Feven and Savransky, Anya and Adhikari, Bhim and Chen, Shuo and Paciga, Sara A. and Whelan, Christopher D. and Xie, Zhiyong and Hyde, Craig L. and Chen, Xing and Schubert, Christian R. and O'Donnell, Patricio and Hong, L. Elliot},
doi = {10.1001/jamapsychiatry.2017.2228},
file = {::},
issn = {2168-622X},
journal = {JAMA Psychiatry},
pages = {1--9},
pmid = {28768312},
title = {{Association of White Matter With Core Cognitive Deficits in Patients With Schizophrenia}},
url = {http://archpsyc.jamanetwork.com/article.aspx?doi=10.1001/jamapsychiatry.2017.2228},
volume = {21228},
year = {2017}
}
