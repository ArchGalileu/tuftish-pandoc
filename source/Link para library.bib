isbn = {1528-1167 (Electronic) 0013-9580 (Linking)},
issn = {00139580},
journal = {Epilepsia},
keywords = {Epilepsy,Localization,Single photon emission computed tomography,Statistical parametric mapping,Surgery,epilepsy,single photon emission computed,statistical parametric mapping,surgery,tomography},
month = {apr},
