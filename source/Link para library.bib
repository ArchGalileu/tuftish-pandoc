month = {may},
number = {3},
pages = {661--664},
pmid = {17470445},
title = {{The empirical status of the ultra high-risk (prodromal) research paradigm.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2526144{\%}7B{\&}{\%}7Dtool=pmcentrez{\%}7B{\&}{\%}7Drendertype=abstract},
volume = {33},
