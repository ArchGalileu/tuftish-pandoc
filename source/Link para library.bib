journal = {Neuropsychology},
keywords = {*Intelligence,*Memory,*Schizophrenic Psychology,*Verbal Learning,10,1037,Adult,Cognition Disorders/etiology/*psychology,Humans,Neuropsychological Tests,Schizophrenia/*complications/diagnosis,Time Factors,a0014708,as a,doi,dx,first-episode,group,healthy controls,http,it is well established,meta-analysis,neurocognition,org,psychosis,reliably demonstrate performance below,schizophrenia,supp,supplemental materials,that individuals with schizophrenia},
number = {3},
pages = {315--336},
pmid = {19413446},
title = {{Neurocognition in first-episode schizophrenia: A meta-analytic review.}},
